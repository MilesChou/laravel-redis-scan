{
    "name": "mileschou/composer-template",
    "description": "The template for create Composer project",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ext-redis": "*",
        "illuminate/container": "^10 | ^11",
        "illuminate/redis": "^10 | ^11",
        "phpbench/phpbench": "^1.0",
        "phpunit/phpunit": "^9.6 | ^10",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Template\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benchmarks\\": "benchmarks",
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
